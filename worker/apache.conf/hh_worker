WSGIPythonPath /opt/HH/worker
<VirtualHost *:8080>
    ServerName fishbone.me
	ServerAdmin me@fishbone.me
	DocumentRoot /opt/HH/worker
	ErrorLog ${APACHE_LOG_DIR}/hh_worker_error.log

	LogLevel warn

	CustomLog ${APACHE_LOG_DIR}/hh_worker_access.log combined

#    WSGIApplicationGroup %{GLOBAL}


    WSGIScriptAliasMatch . /opt/HH/worker/worker.wsgi.py

    <Directory /opt/HH/worker>
        Order allow,deny
        Allow from all
    </Directory>

</VirtualHost>
